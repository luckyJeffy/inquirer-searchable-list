{
  "name": "@senojs/inquirer-searchable-list",
  "version": "0.0.1",
  "description": "Inquirer list prompt, but searchable.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "keywords": [
    "inquirer",
    "searchable list"
  ],
  "author": {
    "name": "jeff",
    "email": "jeffylaiakanull@gmial.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luckyJeffy/inquirer-searchable-list.git"
  },
  "bugs": {
    "url": "https://github.com/luckyJeffy/inquirer-searchable-list/issues"
  },
  "homepage": "https://github.com/luckyJeffy/inquirer-searchable-list#readme",
  "scripts": {
    "test": "tsc && tsc-alias",
    "clean": "rimraf dist",
    "build-lib": "rollup -c rollup.config.js",
    "build-lib:dev": "rollup -c rollup.config.js --environment development",
    "build": "run-s clean test build-lib",
    "build:dev": "run-s clean test build-lib:dev"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "inquirer": "^9.1.0",
    "figures": "^5.0.0",
    "fuzzy": "^0.1.3",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.1"
  }
}